<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified"/>
        </handlers>
        <httpPlatform processPath="%JAVA_HOME%\bin\java.exe"
                      arguments="&quot;-Djava.net.preferIPv4Stack=true&quot; &quot;-Dswarm.http.port=%HTTP_PLATFORM_PORT%&quot; -jar &quot;%HOME%\site\wwwroot\demo-swarm.jar&quot;">
        </httpPlatform>
    </system.webServer>
</configuration>